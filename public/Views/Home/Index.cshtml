@{
    Cassette.Views.Bundles.Reference("~/app");
    Cassette.Views.Bundles.Reference("~/Content");
    Cassette.Views.Bundles.Reference("~/bundles/BowerComponentStyles");
    Cassette.Views.Bundles.Reference("~/bundles/CommonScript");
    Cassette.Views.Bundles.Reference("~/bundles/BowerComponentScript");
}
<!doctype html>
<html>
<!--<![endif]-->
<head>
    <base href="/Index.cshtml">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>PROTON</title>
    <meta name="description" content="Reach, rapid web development theme based on AngularJS">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
    @Cassette.Views.Bundles.RenderStylesheets()
    @Cassette.Views.Bundles.RenderStylesheets("CommonCss")
</head>
<body data-ng-app="PROTONapp" id="app" class="mainclass" ng-controller="mainController">
    <section data-ng-include=" 'app/common/view/noty.html' "></section>
    <div class="" id="div_maincontainer">
        <section data-ng-include=" 'app/navbar/view/navbar.html' " id="navbar" ng-show="navVisible"></section>
        <nav class="navbar-fixed-top" ng-controller="navBarController" style="background-color:#FFD24D"
             ng-show="navLoginVisible">
            <div class="navbar-header">
                <img src="app/img/Logo/Logo.png" style="padding:5px 15px 5px 15px">
            </div>
        </nav>
        <div id="" class="content-container">
            @*<section data-ng-include=" 'app/navbar/view/pushslidenavbar.html' " id="navbar" ng-show="navVisible"></section>*@
            <div class="col-md-12 col-xs-12 col-sm-12">
                <div class="row">
                    </div>
                    <ol class="{{breadcrumbClass}}" style="list-style-type: none;">
                        <span style="color: #999999;" ng-bind="GetParentMenu()"></span>
                        <li ng-repeat="breadcrumb in breadcrumbs.get() track by breadcrumb.path" ng-if="breadcrumb.label"
                            ng-class="{ active: $last }">
                            <a ng-if="!$last" ng-href="#{{ breadcrumb.path }}" ng-bind="breadcrumb.label">
                            </a><span ng-if="$last" ng-bind="breadcrumb.label"></span>
                        </li>
                    </ol>
                <section data-ng-view class="view-container"></section>
            </div>
        </div>
        <section data-ng-include=" 'app/common/view/footer.html' " id="footer"></section>
    </div>
    <!-- /container -->
    @Cassette.Views.Bundles.RenderScripts("CommonScript")
    @Cassette.Views.Bundles.RenderScripts("BowerComponentScript")
    @Cassette.Views.Bundles.RenderScripts("AppJs")
    <!-- /container -->
    <div class="spinner" ng-show="httpRequestloading" id="spinnerModal">
        <div class="httpRequest-loader">
            <i class="fa fa-spinner fa-spin fa-5x"></i>
        </div>
    </div>
</body>
</html>
